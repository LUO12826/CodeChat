<Page
    x:Class="CAC.client.testPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:richText="using:RichTextControls"
    xmlns:monacoEditor="using:Monaco"
    xmlns:code="using:CAC.client.CodeEditorPage"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
    <Page.Resources>


    </Page.Resources>
    
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <ContentPresenter x:Name="testPre"/>
        <StackPanel Orientation="Horizontal" Grid.Row="1">
            <Button Height="30"  Content="switch" Tapped="Button_Tapped"/>
            <Button Height="30" Grid.Row="1" Content="close" Tapped="Button_Tapped_1"/>
        </StackPanel>
        
    </Grid>
</Page>
